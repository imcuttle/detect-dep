{
  "name": "detect-dep",
  "version": "0.3.1",
  "description": "Detect the dependencies of import/require by walking AST",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "doc": "documentation --github --markdown-toc=false readme index.js -a public -s \"API\" && git add README.md",
    "prepublishOnly": "npm test",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "version": "npm run changelog && npm run doc"
  },
  "bin": "bin.js",
  "keywords": [
    "imcuttle",
    "detect-dep"
  ],
  "files": [
    "lib",
    "index.js"
  ],
  "author": "imcuttle",
  "license": "MIT",
  "dependencies": {
    "array-uniq": "^1.0.3",
    "babel-traverse": "^6.26.0",
    "babylon": "^6.18.0",
    "concat-stream": "^1.6.2",
    "meow": "^5.0.0",
    "resolve": "^1.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@moyuyc/husky": "^1.0.0",
    "conventional-changelog-cli": "^2.0.5",
    "documentation": "^5.4.0",
    "jest": "^22.3.0"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imcuttle/detect-dep.git"
  },
  "bugs": {
    "url": "https://github.com/imcuttle/detect-dep/issues"
  },
  "homepage": "https://github.com/imcuttle/detect-dep#readme",
  "husky": {
    "installType": "append",
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
